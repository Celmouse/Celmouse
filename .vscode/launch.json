{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter",
      "type": "dart",
      "request": "launch",
      "program": "flutter/lib/main.dart",
      "deviceId": "iPhone",
    },
    {
      "name": "Electron",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/server",
      "runtimeExecutable": "${workspaceFolder}/server/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/server/node_modules/.bin/electron.cmd"
      },
      "args": [
        "."
      ],
      "outputCapture": "std"
    }
  ]
}